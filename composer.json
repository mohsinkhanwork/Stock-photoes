{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "caouecs/laravel-lang": "~7.0",
        "coderello/laravel-nova-lang": "^1.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.7",
        "laravel/framework": "^7.24",
        "laravel/nova": "*",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "laravelcollective/html": "^6.1",
        "m-a-k-o/nova-custom-table-card": "^3.2",
        "mcamara/laravel-localization": "^1.5",
        "metaregistrar/php-epp-client": "^1.0",
        "michielkempen/nova-order-field": "^2.0",
        "pragmarx/google2fa-laravel": "^1.4",
        "titasgailius/search-relations": "^1.0",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-oracle": "^9.21",
        "laravel/installer": "^1.3 !=1.3.2"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helper/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [{
        "type": "path",
        "url": "./nova"
    }]
}
